{
  "name": "zhipu-copilot-gateway",
  "version": "1.0.0",
  "description": "Ollama-compatible gateway to Zhipu GLM models",
  "main": "dist/cli.js",
  "bin": {
    "zhipu-copilot-gateway": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
