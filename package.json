  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^1.6.0",
    "copyfiles": "^2.4.1",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
